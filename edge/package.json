{
  "name": "edgelambda",
  "version": "1.0.0",
  "description": "Edge lambda",
  "main": "./index.js",
  "private": true,
  "scripts": {
    "audit-checker": "check-audit",
    "audit-resolver": "resolve-audit",
    "build": "npm run build:clean && npm run build:dist && npm run copy-deps",
    "build:clean": "rimraf dist",
    "build:dist": "tsc",
    "copy-deps": "cp package*.json ./dist",
    "copy-deps:ci": "cp package*.json ./dist && (cd ./dist && npm ci --production)",
    "format": "npm run format:prettier",
    "format:prettier": "prettier --write '**/*.{ts,js,md,json}'",
    "test": "mocha test",
    "test:cov": "nyc npm test",
    "lint": "npm run lint:mdlint && npm run lint:eslint",
    "lint:mdlint": "markdownlint '*.md'",
    "lint:eslint": "eslint --cache --ext .ts ./",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.7",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "aws-sdk": "^2.814.0",
    "aws-sdk-mock": "*",
    "babel-plugin-module-resolver": "^3.2.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-mocha": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.1",
    "markdownlint": "^0.17.2",
    "mocha": "^6.2.2",
    "npm-audit-resolver": "^2.1.0",
    "prettier": "^1.19.1",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "na"
  },
  "author": "",
  "license": "MIT",
  "readme": "../README.md"
}
